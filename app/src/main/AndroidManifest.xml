<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wgu.c_196">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.C196">
        <activity android:name=".TermDetailActivity"
            android:label="Term Detail"
            android:parentActivityName=".AllTermsActivity" ></activity>
        <activity
            android:name=".AddTermActivity"
            android:label="Create Term"
            android:parentActivityName=".AllTermsActivity" />
        <activity
            android:name=".AllTermsActivity"
            android:label="All Terms"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyTermActivity"
            android:label="My Term"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>